der() {\r\n        // const movie = movies.results ;\r\n        // console.log(movie);\r\n    \r\n        let genreids = { 28: 'Action', 12: 'Adventure', 16: 'Animation', 35: 'Comedy', 80: 'Crime', 99: 'Documentary',  18: 'Drama', 10751: 'Family', 14: 'Fantasy',\r\n        36: 'History', 27: 'Horror', 10402: 'Music', 9648: 'Mystery', 10749: 'Romance', 878: 'Sci-fi', 10770: 'TV', 53: 'Thriller', 10752: 'War', 37: 'Western' };\r\n\r\n        let filterArr = [];\r\n       \r\n        if(this.state.currText==''){\r\n            filterArr = this.state.movies ;\r\n        }\r\n        else{\r\n           filterArr = this.state.movies.filter((movieobj)=>{\r\n             let title = movieobj.original_title.toLowerCase();\r\n             return title.includes(this.state.currText.toLocaleLowerCase())\r\n\r\n           })\r\n        }\r\n\r\n\r\n        // if(this.state.currgen === \"All Genres\"){\r\n        //     filterArr = this.state.movies;\r\n        // }\r\n        if(this.state.currgen !== \"All Genres\"){\r\n            filterArr = this.state.movies.filter((movieobj)=>genreids[movieobj.genre_ids[0]] == this.state.currgen)\r\n        }\r\n\r\n        let pages =  Math.ceil(filterArr.length/this.state.limit);\r\n        let pagesarr = [];\r\n        for(let  i =1 ; i<=pages ;i++){\r\n            pagesarr.push(i);\r\n        }\r\n        let si = (this.state.currPage-1)*(this.state.limit) ;\r\n        let ei = si+(this.state.limit) ;\r\n        filterArr = filterArr.slice(si,ei);\r\n\r\n\r\n    return (\r\n        <div>\r\n        <>\r\n        <div className=\"main\">\r\n                <div className=\"row\">\r\n                    <div className=\"col-lg-3 col-sm-12\" >\r\n                        <ul class=\"list-group favourites-genres\">\r\n                        {\r\n                            this.state.genres.map((genre)=>(\r\n                                this.state.currgen === genre ?\r\n                                <li class=\"list-group-item\" style=